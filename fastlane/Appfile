app_identifier "com.appunite.shoutit" # The bundle identifier of your app
apple_id "shoutit@appunite.com" # Your Apple email 

for_lane :testflight do 
	app_identifier "com.shoutit-iphone"
	apple_id "shoutit@appunite.com"
end

for_lane :local do 
	app_identifier "com.shoutit-iphone.local"
	apple_id "shoutit@appunite.com"
end

for_lane :production do 
	app_identifier "com.shoutit-iphone"
	apple_id "shoutit@appunite.com"
end

